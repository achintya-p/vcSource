# API Keys for Real Data Sources
LINKEDIN_EMAIL=your_linkedin_email@example.com
LINKEDIN_PASSWORD=your_linkedin_password

# Crunchbase API (if you have access)
CRUNCHBASE_API_KEY=your_crunchbase_api_key

# OpenAI API for enhanced analysis
OPENAI_API_KEY=your_openai_api_key

# Web scraping settings
USE_MOCK_DATA=false
ENABLE_REAL_SCRAPING=true

# RAG Settings
RAG_ENABLED=true
RAG_CACHE_DURATION=3600  # 1 hour cache
RAG_MAX_RETRIES=3

# Rate limiting
RATE_LIMIT_DELAY=2  # seconds between requests
MAX_CONCURRENT_REQUESTS=5

# Database settings (if using)
DATABASE_URL=sqlite:///vc_sourcing.db

# Logging
LOG_LEVEL=INFO 